{
  "name": "Model_Generator",
  "dockerComposeFile": ["../docker-compose.yml"],
  "service": "model-generator",
  // "shutdownAction": "none",
  "customizations": {
    "vscode": {
      "extensions": ["ms-python.python","ms-toolsai.jupyter","donjayamanne.git-extension-pack"]
